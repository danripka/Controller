{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ (GDB)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/GameController",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "make",
            "serverStarted": "",
            "filterStderr": false,
            "filterStdout": false,
            "showDisplayString": true
        }
    ]
}